int getPairsCount(int arr[], int n, int k) {
        int f[1000006]={0};
        long long int count=0;
        for(int i=0;i<n;i++){
            if((k-arr[i])>=0 && (k-arr[i])<=1000005 && f[k-arr[i]]>=1 ){
                count+=f[k-arr[i]];
                f[arr[i]]++;
            }
            else{
                f[arr[i]]++;
            }
        }
        return count;
    }
